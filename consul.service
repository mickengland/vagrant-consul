[Unit]
    Description=Consul Server
    After=network.target

    [Service]
    User=root
    Group=root
    Environment="GOMAXPROCS=2"
    ExecStart=/usr/local/bin/consul agent -config-dir /etc/consul.d/config.json
    ExecReload=/bin/kill -9 $MAINPID
    KillSignal=SIGINT
#    Restart=on-failure


    [Install]
    WantedBy=multi-user.target
